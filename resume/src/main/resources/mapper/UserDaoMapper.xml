<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jw.resume.sys.dao.UserDao">
    <select id="selectUserByname" resultType="com.jw.resume.sys.pojo.User">
        select name,password from t_user where name=#{username} and password=#{password}
    </select>
    <select id="selectUserNameIfThere" resultType="com.jw.resume.sys.pojo.User">
        select name,password from t_user where name=#{username}
    </select>
    <insert id="saveUser">
        insert into t_user (name,password)values (#{username},#{password})
    </insert>
</mapper>